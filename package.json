{
  "name": "react-repo",
  "private": true,
  "description": "A Monorepo for Ant Design customized components with React 19 and TypeScript",
  "scripts": {
    "dev": "lerna run dev --parallel",
    "build": "lerna run build",
    "test": "jest",
    "lint": "eslint \"packages/**/*.{ts,tsx}\"",
    "format": "prettier --write \"packages/**/*.{ts,tsx,json,md}\"",
    "example": "pnpm --filter example dev"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lerna": "^8.1.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
